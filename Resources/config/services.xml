    <parameters>
        <parameter key="anchovy.curl.class">Anchovy\CURLBundle\CURL\Curl</parameter>
    </parameters>

    <services>
        <service id="anchovy.curl" class="%anchovy.curl.class%">
        </service>
    </services>
</container>